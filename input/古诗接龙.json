[
    {
        "author": "孟浩然",
        "paragraphs": [
            "春眠不覺曉，處處聞啼鳥。",
            "夜來風雨聲，花落知多少。"
        ],
        "tags": [
            "唐诗三百首",
            "春",
            "写景",
            "一年级下册",
            "惜春",
            "五言绝句",
            "小学古诗"
        ],
        "title": "春曉",
        "id": "cb168b3b-d104-4df7-9868-1e1225ddb941",
        "time": "唐"       
    },
    {
        "author": "朱熹",
        "paragraphs": [
            "少年易老學難成，一寸光陰不可輕。",
            "未覺池塘春草夢，階前梧葉已秋聲。"
        ],
        "title": "勸學",
        "time": "宋",
        "id": ""
    },
    {
        "author": "李清照",
        "paragraphs": [
            "生當作人傑，死亦爲鬼雄。",
            "至今思項羽，不肯過江東。"
        ],
        "tags": [
            "爱国",
            "励志",
            "典故",
            "古诗三百首",
            "宋诗",
            "咏史怀古",
            "带有地名",
            "宋・辽・金・元",
            "地名"
        ],
        "title": "夏日絕句",
        "id": "ba2fe8ca-41aa-466a-b268-fa3526daafb5",
        "time": "宋"        
    },    
    {
        "author": "羅與之",
        "paragraphs": [
            "東風滿天地，貧家獨無春。",
            "負薪花下過，燕語似譏人。"
        ],
        "title": "商歌三首  其一",
        "id": "e97a3f2d-9ea6-4b60-ab01-3edb49eb2cb1",
        "time": "宋"
    },
    {
        "author": "白居易",
        "paragraphs": [
            "人間四月芳菲盡，山寺桃花始盛開。",
            "長恨春歸無覓處，不知轉入此中來。"
        ],
        "tags": [
            "花",
            "写景",
            "写花",
            "植物"
        ],
        "title": "大林寺桃花",
        "id": "f5f726ac-2edd-44c1-bebf-1cc8476fad4c",
        "time": "唐"
    },
    {
        "author": "李映",
        "paragraphs": [
            "來客踏清溪，放歌伴玉笛。",
            "袖舞晚霞輝，聲敲青山碧。"
        ],
        "title": "山間客訪",
        "time": "五代",
        "id": ""
    },
    {
        "author": "賀知章",
        "paragraphs": [
            "碧玉妝成一樹高，萬條垂下綠絲縧。",
            "不知細葉誰裁出，二月春風似剪刀。"
        ],
        "tags": [
            "春天",
            "写景",
            "咏物",
            "写风",
            "柳树",
            "植物",
            "风",
            "三年级下册",
            "咏物诗",
            "春",
            "小学古诗"
        ],
        "title": "詠柳",
        "id": "bf4ce5bb-7a29-4b33-b8c6-55a020394ab8",
        "time": "唐"
    },    
    {
        "author": "劉駕",
        "paragraphs": [
            "刀劒作鋤犂，耕田古城下。",
            "高秋禾黍多，無地放羊馬。"
        ],
        "title": "唐樂府十首 田西邊",
        "id": "dc3cca75-f9f7-432f-bd1a-0e2ac34e56ff",
        "time": "唐"
    },
    {
        "author": "劉禹錫",
        "paragraphs": [
            "馬踏塵上霜，月明江頭路。",
            "行人朝氣銳，宿鳥相辭去。",
            "流水隔遠村，縵山多紅樹。",
            "悠悠關塞內，往來無閑步。"
        ],
        "title": "途中早發",
        "id": "e5f6df2d-9c5b-460c-985c-c4f6513b40a9",
        "time": "唐"
    },
    {
        "author": "羅隱",
        "paragraphs": [
            "不論平地與山尖，無限風光盡被占。",
            "採得百花成蜜後，爲誰辛苦爲誰甜。"
        ],
        "tags": [
            "咏物诗",
            "怜悯",
            "寓言",
            "咏物"
        ],
        "title": "蜂",
        "id": "f7513920-9e51-4c34-ae10-8ab77d73fb07",
        "time": "唐"
    },
    {
        "author": "韓愈",
        "paragraphs": [
            "天街小雨潤如酥，草色遙看近却無。",
            "最是一年春好處，絕勝煙柳滿皇都。"
        ],
        "tags": [
            "写雨",
            "春天",
            "八年级下册(课外)",
            "写景",
            "柳树",
            "初中古诗",
            "雨",
            "春"
        ],
        "title": "早春呈水部張十八員外二首 一",
        "id": "d7cb4244-143b-403b-8599-b10c6763d032",
        "time": "唐"
    },
    {
        "author": "王維",
        "paragraphs": [
            "獨在異鄉爲異客，每逢佳節倍思親。",
            "遙知兄弟登高處，遍插茱萸少一人。"
        ],
        "tags": [
            "唐诗三百首",
            "七言绝句",
            "思乡",
            "三年级上册",
            "重阳节",
            "小学古诗"
        ],
        "title": "九月九日憶山東兄弟",
        "id": "6bbd1c2c-8c43-4ffb-b7b3-506d74266cee",
        "time": "唐"
    },
    {
        "author": "李昂 柳公權",
        "paragraphs": [
            "人皆苦炎熱，我愛夏日長。",
            "熏風自南來，殿閣生微涼。"
        ],
        "title": "夏日聯句",
        "time": "唐",
        "id": ""
    },
    {
        "author": "杜甫",
        "paragraphs": [
            "兩箇黃鸝鳴翠柳，一行白鷺上青天。",
            "窗含西嶺千秋雪，門泊東吳萬里船。"
        ],
        "tags": [
            "写雪",
            "春",
            "写景",
            "雪",
            "二年级下册",
            "写鸟",
            "小学古诗",
            "鸟"
        ],
        "title": "絕句四首 三",
        "id": "abf4daf0-0c7b-4b0c-87a2-2c48097d6a65",
        "time": "唐"
    },
    {
        "author": "李白",
        "paragraphs": [
            "船下廣陵去，月明征虜亭。",
            "山花如繡頰，江火似流螢。"
        ],
        "title": "夜下征虜亭",
        "id": "7f066769-61bb-469b-8224-e90424e5cddf",
        "time": "唐"
    },
    {
        "author": "葉紹翁",
        "paragraphs": [
            "應憐屐齒印蒼苔，小扣柴扉久不開。",
            "春色滿園關不住，一枝紅杏出牆來。"
        ],
        "tags": [
            "春天",
            "寫景",
            "遊園",
            "古詩三百首",
            "哲理",
            "宋詩",
            "春"
        ],
        "title": "遊園不值",
        "id": "53d114ff-35cf-47c1-af7b-362be74e63fd",
        "time": "宋"
    },
    {
        "author": "韋冰",
        "paragraphs": [
            "來時歡笑去時哀，家國迢迢向越臺。",
            "待寫百年幽思盡，故宮流水莫相催。"
        ],
        "title": "和三鄉詩",
        "id": "83a8c92c-5c97-469b-a2aa-e1c411934cf0",
        "time": "唐"
    },
    {
        "author": "虞世南",
        "paragraphs": [
            "垂緌飲清露，流響出疏桐。",
            "居高聲自遠，非是藉秋風。"
        ],
        "tags": [
            "咏物诗",
            "寓理",
            "咏物"
        ],
        "title": "蟬",
        "id": "f8998cfc-470f-45e6-a06c-5e30c18c0645",
        "time": "唐"
    },
    {
        "author": "杜甫",
        "paragraphs": [
            "風急天高猨嘯哀，渚清沙白鳥飛迴。",
            "無邊落木蕭蕭下，不盡長江衮衮來。",
            "萬里悲秋常作客，百年多病獨登臺。",
            "艱難苦恨繁霜鬢，潦倒新停濁酒桮。"
        ],
        "tags": [
            "唐诗三百首",
            "长江",
            "七言律诗",
            "写景",
            "忧国忧民",
            "秋天",
            "登高",
            "抒情"
        ],
        "title": "登高",
        "id": "a4d718ee-c020-42b4-97df-499cd0076f7b",
        "time": "唐"
    },
    {
        "author": "王安石",
        "paragraphs": [
            "北山輸綠漲橫陂，直壍回塘灧灧時。",
            "細數落花因坐久，緩尋芳草得歸遲。"
        ],
        "title": "北山",
        "id": "7c8855c8-c2a0-418f-a1ef-a562ea762306",
        "time": "宋"
    }
]